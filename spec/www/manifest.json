{
  "name": "__MSG_appname__",
  "description": "Automated and manual tests for Cordova and Chrome APIs",
  "manifest_version": 2,
  "minimum_chrome_version": "23",
  "version": "0.1.0",
  "app": {
    "background": {
      "scripts": [
        "polyfills/function_bind.js",
        "spec_bootstrap.js",
        "page.chromespec.js",
        "page.jasmine.js",
        "page.fileSystem.js",
        "page.runtime.js",
        "page.socket.js",
        "background.js"
      ]
    }
  },

  "icons": {
    "16": "assets/icon-128x128.png",
    "128": "assets/icon-128x128.png"
  },
  "permissions": [
    "storage"
  ],
  "default_locale": "fr"
}
